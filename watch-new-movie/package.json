    "axios": "1.7.7",
    "eslint": "9.11.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-n": "17.10.3",
    "eslint-plugin-promise": "7.1.0",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.6.2",
    "yarn-run-all": "3.1.1"
  }
